<aiml>

	<topic name="FEEL">
	    <category>
	        <pattern>#</pattern>
	        <template>
	            <think>
	                <set name="feel"><star /></set>
	                <set name="topic"></set>
	            </think>
	            <condition name="feel">
	                <li value="fine"><think><set name="topic">DOING</set></think>Perfect! What are you doing?</li>
	                <li value="good"><think><set name="topic">DOING</set></think>Nice! What are you doing?</li>
	                <li value="bad">Well, get better soon then!</li>
	                <li><think><set name="topic">DOING</set></think>Okay! What are you doing?</li>
	            </condition>
	        </template>
	    </category>
	</topic>

